{
  "name": "vg_map",
  "title": "vg_map",
  "summary": "vg_map",
  "dxapi": "1.0.0",
  "version": "0.0.1",
  "inputSpec": [
    {
      "name": "reads",
      "class": "file",
      "patterns": ["*.fastq.gz", "*.fq.gz"]
    },
    {
      "name": "reads2",
      "class": "file",
      "optional": true,
      "patterns": ["*.fastq.gz", "*.fq.gz"]
    },
    {
      "name": "vg_indexed_tar",
      "class": "file",
      "optional": false,
      "type": "vg_indexed_tar"
    },
    {
      "name": "map_options",
      "class": "string",
      "default": "-FX 1.8 -B 100000"
    },
    {
      "name": "vg_exe",
      "class": "file",
      "type": "vg_exe",
      "default": {"$dnanexus_link": "VG_EXE_DXID"}
    }
  ],
  "outputSpec": [
    {
      "name": "gam",
      "class": "file",
      "patterns": ["*.gam"]
    }
  ],
  "runSpec": {
    "interpreter": "bash",
    "file": "src/code.sh",
    "execDepends": [{"name": "libjansson4"}],
    "systemRequirements": {
      "main": {
        "instanceType": "mem3_ssd1_x16"
      }
    }
  },
  "access": {
    "network": ["*"]
  },
  "authorizedUsers": []
}
